{
  "env": {
    "node": true,
    "mocha": true,
    "browser": true,
    "builtin": true
  },
  "ecmaFeatures": {
    "modules": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "octalLiterals": true,
    "restParams": true,
    "spread": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true,
    "experimentalObjectRestSpread": true
  },
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 2,
    "curly": [ 2, "multi-line" ],
    "dot-notation": [ 2, { "allowKeywords": true } ],
    "eqeqeq": [ 2, "allow-null" ],
    "strict": [ 2, "global" ],
    "guard-for-in": 0,
    "new-cap": [2, { "capIsNewExceptions": ["Model", "View", "TROGDOR"] }],
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [ 2, "except-parens" ],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-undef": 2,
    "no-unused-vars": [2, { "vars": "all", "args": "none" }],
    "no-with": 2,
    "quotes": [ 2, "single" ],
    "semi": [ 2, "always" ],
    "space-after-keywords": [ 2, "always" ],
    "valid-typeof": 2,
    "wrap-iife": [ 2, "inside" ]
  }
}
